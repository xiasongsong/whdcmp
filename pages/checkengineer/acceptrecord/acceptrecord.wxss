.card{
  background: #fff;
  margin:20rpx 0;
  min-height: 30rpx;
  padding: 15rpx 0;
}
.card:first-child{
  margin-top: 0;
}
.card .title{
  padding: 15rpx 30rpx;
  font-size: 30rpx;
  color:#666;
  border-bottom: 1rpx solid #ddd;
}
.p{
  font-size: 26rpx;
  padding:0 30rpx;
  margin:15rpx 0;
  color: #333;
}
.p .fh{
  width: 150rpx;
  font-weight: 200;
}
.p .fb{
  flex: 1;
  text-align: right;
}